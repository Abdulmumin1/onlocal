{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "identifier": "com.onlocal.app",
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "OnLocal",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "capabilities": [
        "main-capability"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "OnLocal",
  "version": "0.1.0"
}
